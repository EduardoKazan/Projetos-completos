      "compilerPath": "C:/xtensa-esp32-elf/bin/xtensa-esp32-elf-gcc.exe",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "windows-gcc-x64",
      "browse": {
        "path": [
          "${workspaceFolder}/main",
          "${workspaceFolder}/esp_idf_components",
          "${env:IDF_PATH}/components"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    }
